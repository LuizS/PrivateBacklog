    <div class="col-md-3" role="complementary">
        <div class="bs-sidebar">
            <ul class="nav bs-sidenav">
                {{#each item in model}}
                    <li {{bind-attr class="item.Active:active"}}>{{#link-to 'list' item.Name}} {{item.Name}} {{item.Active}} {{/link-to}}
                        <ul>
                            {{#each resource in item.Items}}
                                <li class="resourceItem">
                                    {{#link-to 'resource' item.Name resource.Id}} {{resource.Name}} {{/link-to}}
                                </li>
                            {{/each}}
                        </ul>
                    </li>
                {{/each}}
            </ul>
        </div>
    </div>
